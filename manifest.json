{
  "manifest_version": 2,
  "name": "Add Padding",
  "description": "Add padding at bottom of web page for better browsing",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "exclude_matches": ["https://rapidgator.net/*", "https://k2s.cc/*"],
    "css": ["custom.css"]
  }]
}